{
    "create r code chunk": {
        "prefix": "rc",
        "body": [
            "```{r}",
            "$0",
            "```"
        ],
        "description": "create r code chunk"
    }, 


    "create r plot code chunk": {
        "prefix": "rp",
        "body": [
            "```{r plot, fig.align = 'center', fig.height=3, fig.cap = '', out.width='100%'}",
            "$0",
            "```"
        ],
        "description": "create r plot code chunk"
    },

    "create rmd meta data": {

        "prefix": "yaml",
        "body": [
            "---",
            "title: 'rmd title'",
            "author: 'Rongzhao Yan'",
            "date: '12/14/2021'",
            "output: ",
            "    bookdown::pdf_document2:",
            "      toc: False",
            "      includes:",
            "            in_header: tem.tex",
            "bibliography: citation.bib",
            "---",
            "\n",
            "```{r setup, include=FALSE}",
            "knitr::opts_chunk\\$set(echo = TRUE, warning = F, message = F)",
            "options(scipen = 10)",
            "```",
            "$0"
        ],
        "description": "create rmd metadata"
    },

    "create rmd beamer meta data": {

        "prefix": "beamer",
        "body": [
            "---",
            "title: 'rmd title'",
            "author: 'Rongzhao Yan'",
            "date: '12/14/2021'",
            "fontsize: 10pt",
            "output: ",
            "    beamer_presentation:",
            "      latex_engine: xelatex",
            "      theme: 'metropolis'",
            "      colortheme: 'spruce'",
            "      keep_tex: true",
            "      includes:",
            "            in_header: env.tex",
            "bibliography: citation.bib",
            "classoption: 'handout'",
            "---",
            "\n",
            "```{r setup, include=FALSE}",
            "knitr::opts_chunk\\$set(echo = TRUE, warning = F, message = F)",
            "options(scipen = 10)",
            "```",
            "$0"
        ],
        "description": "create rmd beamer metadata"
    },

    "pipe symbol": {
        "prefix": "pip",
        "body": [
            "%>% $0"
        ],
        "description": "pipe"
    },

    "i.i.d symbol": {
        "prefix": "iid",
        "body": ["\\overset{\\mathrm{iid}}{\\sim} $0"
        ],
        "description": "iid from"
    },

    "assignment symbol": {
        "prefix": "eq",
        "body": ["<- $0"],
        "description": "assignment symbol"
    },

    "reference block": {
        "prefix": "refchap",
        "body": [
            "# References",
            "\n",
            "<div id=\"refs\"></div>",
            "$0"
        ]
    },

    "\\begin{}â€¦\\end{}": {
        "prefix": "begin",
        "body": [
            "\\\\begin{${1:env}}",
            "\t${1/(enumerate|itemize|list)|(description)|.*/(?1:\\item )(?2:\\item)/}$0",
            "\\\\end{${1:env}}"
        ],
        "description": "Begin - End"
    },

    "cross reference": {
        "prefix": "ref",
        "body": [
            "\\ref{${1:fig}:$2}$0"
        ],
        "description": "Reference"
    },

    "Figure": {
        "prefix": "figure",
        "body": [
            "\\begin{figure}",
            "\t\\begin{center}",
            "\t\t\\includegraphics[width=1\\textwidth]{figures/$1}",
            "\t\\end{center}",
            "\t\\caption{$3}",
            "\t\\label{fig:$4}",
            "\\end{figure}",
            "$0"
        ],
        "description": "Add a figure"
    },

    "command": {
        "prefix": "cmd",
        "body": [
            "\\\\$1{$0}"
        ],
        "description": "insert a command"
    },

    "mathbf": {
        "prefix": "mbf",
        "body": [
            "\\\\mathbf{$0}"
        ],
        "description": "mathbf"
    },

    "mathbb": {
        "prefix": "mbb",
        "body": [
            "\\\\mathbb{$0}"
        ],
        "description": "mathbb"
    },

    "boldsymbol": {
        "prefix": "boldsymbol",
        "body": [
            "\\\\boldsymbol{$0}"
        ],
        "description": "boldsymbol"
    },

    "mathrm": {
        "prefix": "mrm",
        "body": [
            "\\\\mathrm{$0}"
        ],
        "description": "mathrm"
    },

    "mathcal": {
        "prefix": "mcal",
        "body": [
            "\\\\mathcal{$0}"
        ],
        "description": "mathcal"
    }

}
