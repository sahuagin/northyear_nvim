{
    "create r code chunk": {
        "prefix": "rc",
        "body": [
            "```{r}",
            "$0",
            "```"
        ],
        "description": "create r code chunk"
    },

    "create python code chunk": {
        "prefix": "pyc",
        "body": [
            "```{python}",
            "$0",
            "```"
        ],
        "description": "create r code chunk"
    },

    "create python plot chunk": {
        "prefix": "pyp",
        "body": [
            "```{python}",
            "#| label: fig-figure",
            "#| fig-cap: 'caption of figure'",
            "#| fig-align: 'center'",
            "#| fig-width: 6",
            "#| fig-height: 3",
            "#| fig-format: 'jpeg'",
            "#| fig-dpi: 300",
            "#| out-width: '100%'",
            "$0",
            "```"
        ],
        "description": "create python plot code chunk"
    },

    "create r plot code chunk": {
        "prefix": "rp",
        "body": [
            "```{r}",
            "#| label: fig-figure",
            "#| fig-cap: 'caption of figure'",
            "#| fig-align: 'center'",
            "#| fig-width: 6",
            "#| fig-height: 3",
